{
  "name": "tfjs-examples",
  "version": "0.1.0",
  "description": "TenserFlos.js Examples",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/tfjs-examples.git"
  },
  "keywords": [
    "TenserFlow"
  ],
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/tfjs-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/tfjs-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "scripts": {
    "eslint": "eslint . --ext .js,.jsx --cache",
    "eslint-fix": "eslint . --ext .js,.jsx --cache --fix",
    "prettier": "prettier --list-different '**/*.{json,js,jsx,ts,tsx}'",
    "prettier-fix": "prettier --write '**/*.{json,js,jsx,ts,tsx}'",
    "validate": "npm run eslint && npm run prettier"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.0"
  }
}
