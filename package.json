{
  "name": "tfjs-examples",
  "version": "0.1.0",
  "description": "TenserFlos.js Examples",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickbeerendonk/tfjs-examples.git"
  },
  "keywords": [
    "TenserFlow"
  ],
  "author": "Rick Beerendonk",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/rickbeerendonk/tfjs-examples/issues"
  },
  "homepage": "https://github.com/rickbeerendonk/tfjs-examples#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate",
      "pre-push": "npm run validate"
    }
  },
  "scripts": {
    "eslint": "eslint . --ext .js,.html --cache",
    "eslint-fix": "eslint . --ext .js,.html --cache --fix",
    "prettier": "prettier --list-different '**/*.{json,js,html}'",
    "prettier-fix": "prettier --write '**/*.{json,js,html}'",
    "validate": "npm run eslint && npm run prettier"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^1.4.0"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-plugin-html": "^6.0.0",
    "husky": "^3.1.0",
    "prettier": "^1.19.1"
  }
}
